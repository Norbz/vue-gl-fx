  "private": false,
  "license": "MIT",
  "description": "A simple WebGL components for accelerated 2D filter or effects. Can be used with Shadertoys codes",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.umd.js",
  "author": "Nicolas ChesnÃ© & Laurent Gabarre",
  "scripts": {
    "dev": "bili && ./node_modules/poi/bin/cli.js",
    "build:demo": "./node_modules/poi/bin/cli.js build",
    "build:demo:gh": "./node_modules/poi/bin/cli.js build --public-path /vue-gl-fx/",
    "publish:demo": "npm run build:demo:gh && gh-pages -d demo",
    "build": "rm -rf ./dist && bili --env.NODE_ENV production --bundle-node-modules true",
