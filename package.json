  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "babel-eslint": "^10.0.3",
    "bili": "^4.8.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^4.5.0",
    "gh-pages": "^2.1.1",
    "poi": "^10.2.7",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-vue": "^5.1.1",
    "stylus": "^0.54.7",
